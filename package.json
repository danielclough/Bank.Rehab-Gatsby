{
  "name": "@windedge/gatsby-tailwind-serif",
  "description": "Gatsby starter styled with Tailwind",
  "version": "1.0",
  "author": "@windedge",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "2.20.10",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-manifest": "2.3.3",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-postcss": "^2.2.1",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "graphql": "14.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "prettier": "1.18.2",
    "tailwindcss": "1.2.0",
    "tailwindcss-transforms": "^2.2.0"
  },
  "Repository": {
    "type": "git",
    "url": "https://github.com/windedge/gatsby-tailwind-serif"
  },
  "bugs": {
    "url": "https://github.com/windedge/gatsby-tailwind-serif/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "terser": "4.0.0",
    "terser-webpack-plugin/terser": "4.0.0"
  }
}
